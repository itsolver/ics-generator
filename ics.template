BEGIN:VCALENDAR
VERSION:2.0
PRODID:-//<%= host %>
<% if(name) { %>X-WR-CALNAME:<%= name %><% } %>
X-WR-TIMEZONE:<%= timezone %>
BEGIN:VEVENT
DTSTAMP:<%= now %>
UID:<%= uid %>
<% if (allDay) { %>
DTSTART;VALUE=DATE:<%= startDate %>
DTEND;VALUE=DATE:<%= endDate %>
<% } else { %>
DTSTART;TZID=Etc/GMT:<%= startDate %>
DTEND;TZID=Etc/GMT:<%= endDate %>
<% } %>
SUMMARY:<%= summary %>
DESCRIPTION:<%= description %>
LOCATION:<%= location %>
<% if(rrule) { %>RRULE:<%= rrule %><% } %>
END:VEVENT
END:VCALENDAR
