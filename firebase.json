{
  "hosting": {
    "public": "public",
    "rewrites": [
      {
        "source": "*",
        "function": "app"
      }
    ],
    "cleanUrls": true,
    "trailingSlash": false
  },
  "functions": {
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" run lint"
    ],
    "source": "functions"
  }
}
